{% extends '@SonataAdmin/CRUD/base_list_field.html.twig' %}

{% block field %}
    {% if object.images.first is not empty %}
        <img src="{{ vich_uploader_asset(object.images.first) }}"
             alt="{{ object.images.first.image.name }}"
             style="width:auto; height:80px;"/>
    {% endif %}
{% endblock %}
